
public class SASolver {
	public static void main(String[] args){
		PreferenceTable p = new PreferenceTable("Project allocation data.tsv");
		CandidateSolution s = new CandidateSolution(p);
		int heat = 100;
		System.out.println(s.getEnergy());
		
		while (heat!= 0){
			System.out.println(s.getEnergy());
			if (s.tweak()){
				heat = 100;
			}
			else {
				heat = heat - 1;
			}
		}
	}
}
